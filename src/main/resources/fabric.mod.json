{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "MimiCraft",
  "description": "A mod that expands upon the concept of MimiCream from Alex's Mobs for the benefit of all minecraft creators.",
  "authors": [
    "Dr. Rubisco (@thedocruby)"
  ],
  "contact": {
    "homepage": "https://thedocruby.github.io/MimiCraft",
    "sources": "https://github.com/thedocruby/MimiCraft"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.thedocruby.mimicraft.Mimicraft"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "another-mod": "*"
  }
}
